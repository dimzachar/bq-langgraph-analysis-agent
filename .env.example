# Options: "gemini" or "openrouter"
LLM_PROVIDER=openrouter

# Google Gemini Settings (required if LLM_PROVIDER=gemini)
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_MODEL=gemini-1.5-flash
# OpenRouter Settings (required if LLM_PROVIDER=openrouter)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=xiaomi/mimo-v2-flash:free
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
FALLBACK_MODEL=google/gemini-2.0-flash-exp:free
# BigQuery Settings
GOOGLE_CLOUD_PROJECT=your_gcp_project_id
BIGQUERY_DATASET=bigquery-public-data.thelook_ecommerce

# Agent Settings
MAX_QUERY_RETRIES=2
QUERY_TIMEOUT=60

# Optional: Override suggested models for /model command (comma-separated)
# SUGGESTED_MODELS=model1,model2,model3